  "description": "Convert Obsidian notes with URLs into full-length articles using AI LLM services (Gemini API, Ollama)",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "setup": "node setup.js",
    "start": "node index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
